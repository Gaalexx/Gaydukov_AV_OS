{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug",
      "type": "cppdbg",
      "request": "launch",
      "args": ["10", "1", "100"],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [],
      "externalConsole": false,
      "linux": {
        "MIMode": "gdb",
        "miDebuggerPath": "gdb",
        "program": "${workspaceFolder}/output/main"
      },
      "osx": {
        "MIMode": "lldb",
        "miDebuggerPath": "lldb-mi",
        "program": "${workspaceFolder}/output/main"
      },
      "windows": {
        "MIMode": "gdb",
        "miDebuggerPath": "gdb.exe",
        "program": "${workspaceFolder}/output/main.exe"
      },
      "preLaunchTask": "build"
    },
    {
      "name": "C/C++ Runner: Debug Session",
      "type": "cppdbg",
      "request": "launch",
      "args": ["10", "1", "100"],
      "stopAtEntry": false,
      "externalConsole": false,
      "cwd": "/home/gaalex/Programs/OS/Second_lab",
      "program": "/home/gaalex/Programs/OS/Second_lab/build/Debug/outDebug",
      "MIMode": "gdb",
      "miDebuggerPath": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    }
  ]
}